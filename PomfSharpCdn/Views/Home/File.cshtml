@{
    ViewBag.Title = ViewBag.FileId;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
</head>
<body>
    @{
        if (ViewBag.VideoType)
        {
            <video autoplay controls loop>
                <source src="@ViewBag.FileLocation" type="@ViewBag.FileType"/>
            </video>
        }
        else
        {
            <img src="@ViewBag.FileLocation" />
        }
    }
</body>
</html>
